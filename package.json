{
  "name": "@vuwijs/vuwi",
  "version": "0.0.0",
  "types": "dist/vuwi.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:local": "rm -f vuwijs-vuwi-$npm_package_version.tgz && pnpm build && npm pack && mkdir -p ~/local_modules && rm ~/local_modules/vuwi.tgz && cp vuwijs-vuwi-$npm_package_version.tgz ~/local_modules/vuwi.tgz"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "@vueuse/core": "^7.5.3",
    "maska": "^1.5.0",
    "mitt": "^3.0.0",
    "vee-validate": "^4.5.5",
    "vue": "^3.2.25",
    "vue-demi": "latest",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-dts": "^1.0.4",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-windicss": "^1.6.2",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.4.2"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vuwi.umd.js",
  "module": "./dist/vuwi.es.js",
  "exports": {
    ".": {
      "import": "./dist/vuwi.es.js",
      "require": "./dist/vuwi.umd.js",
      "types": "./dist/vuwi.d.ts"
    }
  }
}
